{
  "name": "honeycomb-project-snippets",
	"displayName": "honeycomb project snippets",
	"description": "honeycomb项目的代码片段",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
  "icon": "https://tva1.sinaimg.cn/large/008i3skNgy1gw0xle4scsj303u042jr6.jpg",
  "keywords": [
    "snippets",
    "react",
    "honeycomb",
    "private project"
  ],
  "publisher": "wqy",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangqinyong1994/hc-snippets.git"
  },
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:extension.ZSearch"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.ZSearch",
				"title": "honeycomb project snippets"
			}
		],
    "keybindings": [
      {
        "command": "extension.ZSearch",
        "key": "ctrl+alt+i",
        "mac": "shift+cmd+i",
        "when": "editorTextFocus"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./src/snippets/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      }
    ]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"ts-loader": "^9.2.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	}
}
